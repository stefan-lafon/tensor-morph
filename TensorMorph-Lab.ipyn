                "# @title 3. Test MLIR\n",
                "%%writefile /content/tensormorph_local/test.mlir\n",
                "module {\n",
                "  func.func @main(\n",
                "    %input: tensor<1x4x4x1xf32>,\n",
                "    %weights: tensor<1x1x1x1xf32>,\n",
                "    %bias_ignored: tensor<1xf32>\n",
                "  ) -> tensor<1x4x4x1xf32> {\n",
                "\n",
                "    // Initial Bias for Conv is 2.0\n",
                "    %c_bias = \"tosa.const\"() {value = dense<2.0> : tensor<1xf32>} : () -> tensor<1xf32>\n",
                "\n",
                "    // Shift/BatchNorm factor is 1.5\n",
                "    %shift = \"tosa.const\"() {value = dense<1.5> : tensor<1xf32>} : () -> tensor<1xf32>\n",
                "\n",
                "    // 1. TOSA Convolution\n",
                "    // All shapes are now synchronized to 1x1x1x1 to ensure type legalization passes.\n",
                "    %0 = \"tosa.conv2d\"(%input, %weights, %c_bias) {\n",
                "      dilation = array<i64: 1, 1>,\n",
                "      pad = array<i64: 0, 0, 0, 0>,\n",
                "      stride = array<i64: 1, 1>\n",
                "    } : (tensor<1x4x4x1xf32>, tensor<1x1x1x1xf32>, tensor<1xf32>) -> tensor<1x4x4x1xf32>\n",
                "\n",
                "    // 2. TOSA Add (The fusion target)\n",
                "    %result = \"tosa.add\"(%0, %shift) : (tensor<1x4x4x1xf32>, tensor<1xf32>) -> tensor<1x4x4x1xf32>\n",
                "\n",
                "    return %result : tensor<1x4x4x1xf32>\n",
                "  }\n",
                "}"
            ],
